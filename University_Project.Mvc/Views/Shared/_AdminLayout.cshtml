<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Complete Responsive Grocery Website Design Tutorial</title>

    <link rel="stylesheet" href="https://unpkg.com/swiper@7/swiper-bundle.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css">

    <link rel="stylesheet" href="@Url.Content("~/css/main.css")">
    @RenderSection("Styles", false)

</head>

<body> 
<!-- header section starts  -->

<header class="header">
    
    <h1 style=""></h1>
    <a href="#" class="logo"> Skates duudeeess </a>

    <h1 style="margin-left: 18%"></h1>
    <nav class="navbar">
        <a asp-area="" asp-controller="Home" asp-action="Index">Home</a>
        <a asp-area="" asp-controller="Home" asp-action="Catalog">Catalog</a>
        <a asp-area="" asp-controller="Home" asp-action="ContactUs">Contact Us</a>
        <a asp-area="" asp-controller="Home" asp-action="AboutUs">About Us</a>
    </nav>

    <h1 style="margin-left: 23%"></h1>
    <div class="icons">
        <div class="fas fa-bars" id="menu-btn"></div>
        <div class="fas fa-user" id="login-btn"></div>
    </div>
    
    <partial name="_LoginPartial" />

</header>

@RenderBody()


<section class="footer">

    <div class="box-container">

        <div class="box">
            <h3> Skate duudeeess</h3>
            <p>some text</p>
            <div class="share">
                <a href="#" class="fab fa-facebook-f"></a>
                <a href="#" class="fab fa-twitter"></a>
                <a href="#" class="fab fa-instagram"></a>
                <a href="#" class="fab fa-linkedin"></a>
            </div>
        </div>

        <div class="box">
            <h3>contact info</h3>
            <a href="#" class="links">+(995) 591-07-80-85 </a>
            <a href="#" class="links">Aceofspades@gmail.com </a>
            <a href="#" class="links">Kutaisi, Georgia</a>
        </div>

        <div class="box">
            <h3>quick links</h3>
            <a href="#" class="links">Home </a>
            <a href="#" class="links">Products </a>
            <a href="#" class="links">Categories </a>
        </div>

    </div>

    <div class="credit"></div>

</section>

<script src="https://unpkg.com/swiper@7/swiper-bundle.min.js"></script>

<!-- Messenger საუბრის დანამატი Code -->
    <div id="fb-root"></div>

    <!-- Your საუბრის დანამატი code -->
    <div id="fb-customer-chat" class="fb-customerchat">
    </div>

    <script>
      var chatbox = document.getElementById('fb-customer-chat');
      chatbox.setAttribute("page_id", "103027109148075");
      chatbox.setAttribute("attribution", "biz_inbox");
    </script>

    <!-- Your SDK code -->
    <script>
      window.fbAsyncInit = function() {
        FB.init({
          xfbml            : true,
          version          : 'v14.0'
        });
      };

      (function(d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) return;
        js = d.createElement(s); js.id = id;
        js.src = 'https://connect.facebook.net/en_US/sdk/xfbml.customerchat.js';
        fjs.parentNode.insertBefore(js, fjs);
      }(document, 'script', 'facebook-jssdk'));
    </script>

<script src="~/js/main.js"></script>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
@RenderSection("Scripts", required: false)

</body>
</html>